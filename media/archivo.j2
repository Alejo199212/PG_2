<DocElectronico>
	<Encabezado>
		<Receptor>
			<NITReceptor>{{nit}}</NITReceptor>
			<Nombre>{{nombre}}</Nombre>
			<Direccion>{{direccion}}</Direccion>
		</Receptor>
		<InfoDoc>
			<TipoVenta>S</TipoVenta>
			<DestinoVenta>1</DestinoVenta>
			<Fecha>{{fecha}}</Fecha>
			<Moneda>1</Moneda>
			<Tasa>1</Tasa>
			<Referencia>{{referencia}}</Referencia>
			<NumeroAcceso/>
			<SerieAdmin>SRV</SerieAdmin>
			<NumeroAdmin>4708</NumeroAdmin>
		</InfoDoc>
		<Totales>
			<Bruto>{{impBruto}}</Bruto>
			<Descuento>0.00</Descuento>
			<Exento>0.00</Exento>
			<Otros>0.00</Otros>
			<Neto>{{totalImpNeto}}</Neto>
			<Isr>0.00</Isr>
			<Iva>{{iva}}</Iva>
			<Total>{{totalFactura}}</Total>
		</Totales>
	</Encabezado>
	<Detalles>
		{% for item in producto %}
          <Productos>
			<Producto>{{item.producto}}</Producto>
			<Descripcion>{{ item.descripcion}}</Descripcion>
			<Medida>{{item.medida}}</Medida>
			<Cantidad>{{item.cantidad}}</Cantidad>
			<Precio>{{item.precioU}}</Precio>
			<PorcDesc>0.00</PorcDesc>
			<ImpBruto>{{ item.ImpBruto}}</ImpBruto>
			<ImpDescuento>0.00</ImpDescuento>
			<ImpExento>0.00</ImpExento>
			<ImpOtros>0.00</ImpOtros>
			<ImpNeto>{{item.ImpNeto}}</ImpNeto>
			<ImpIsr>0</ImpIsr>
			<ImpIva>{{item.iva}}</ImpIva>
			<ImpTotal>{{item.total}}</ImpTotal>
			<TipoVentaDet>{{item.tipo}}</TipoVentaDet>
		  </Productos>
		{% endfor %}
	</Detalles>
</DocElectronico>